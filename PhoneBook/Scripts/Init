Begin TRANSACTION
CREATE TABLE PhoneBooks
(
	PhoneBookId INT NOT NULL IDENTITY(1,1) PRIMARY KEY,
	Name VARCHAR(200) NOT NULL
)
CREATE TABLE Entries
(
	EntryId INT NOT NULL IDENTITY(1,1) PRIMARY KEY,
	PhoneBookId INT NOT NULL FOREIGN KEY REFERENCES PhoneBooks(PhoneBookId),
	Name VARCHAR(200) NOT NULL,
    PhoneNumber VARCHAR(30) NOT NULL,
)
COMMIT TRANSACTION
